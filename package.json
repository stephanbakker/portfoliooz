{
  "name": "portfoliooz",
  "version": "0.1.0",
  "description": "portfolio gallery",
  "main": "server.bundle.js",
  "scripts": {
    "start": "node server.bundle",
        "start:dev": "npm run build:server && nodemon server.bundle.js",
        "start:prod": "npm run build && node server.bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
        "dev:client": "webpack -w",
    "build": "npm run build:client && npm run build:server",
        "build:server": "webpack --config webpack.server.config.js --progress --colors",
        "build:client": "webpack && npm run build:css",
        "build:css": "node-sass --include-path static src/main.scss public/main.css",
    "watch:css": "npm run build:css -- -w",
    "postinstall": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.10.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "parallelshell": "^2.0.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "webpack": "^1.12.14",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "flickrapi": "^0.3.36",
    "markdown": "^0.5.0",
    "mongodb": "^2.0.52",
    "mongoose": "^4.3.6",
    "nconf": "^0.8.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "request": "^2.67.0"
  }
}
