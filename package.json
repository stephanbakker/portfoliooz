    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "parallelshell": "^2.0.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "flickrapi": "^0.3.36",
    "markdown": "^0.5.0",
    "mongodb": "^2.0.52",
    "mongoose": "^4.3.6",
    "nconf": "^0.8.2",
    "react": "^0.14.7",
    "request": "^2.67.0"
  }
