{
  "name": "portfoliooz",
  "version": "1.0.0",
  "description": "portfolio gallery",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --debug server.js && npm run watch:jsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-css": "node-sass --include-path static static/main.scss dist/main.css",
    "watch:jsx": "babel --presets react --watch src/views  --out-dir app/dist/",
    "watch-css": "npm run build-css -- -w",
    "browsersync": "browser-sync start --startPath ./ --server  --files 'dist/*'"
  },
